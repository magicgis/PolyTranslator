[package]
name = "lang-generator"
edition = "2021"
version = "1.0.4"
description = "A procedural macro crate for generating language-related code in poly-translator"
license = "MIT"
readme = "README.md"
# publish = false

[lib]
proc-macro = true

[dependencies]
proc-macro2 = "1.0"
quote = "1.0"
syn = { version = "2.0", features = ["full"] }
reqwest = { version = "0.12", features = ["blocking", "json"] }
csv = "1.3"
serde = { version = "1.0", features = ["derive"] }
heck = "0.5.0"
iso-639 = "0.1.0"
serde_json = "1.0"
rust_iso639 = "0.0.3"
